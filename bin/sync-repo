#!/bin/bash
set -e
BASEDIR="$(dirname "$(dirname "$(readlink -fn "$0")")")"
cd "$BASEDIR"

REMOTEROOT=/home/arch32/webroot/htdocs/artera

for relver in {5..7}; do
	rsync --delete --delete-excluded -av --exclude "*.log" \
	      ./epel-$relver-x86_64/ \
	      repo.artera.ws:$REMOTEROOT/rhel$relver/
done
